{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wiledia/laravel-messenger"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wiledia/laravel-money"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wiledia/laravel5-3-installer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wiledia/backport"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wiledia/themes"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.8.*",
        "electrolinux/phpquery": "0.9.*",
        "guzzlehttp/guzzle": "^6.2",
        "dborsatto/php-giantbomb": "^0.6.0",
        "nesbot/carbon": "^1.21",
        "cknow/laravel-money": "dev-master",
        "tom-lingham/searchy" : "2.*",
        "cmgmyr/messenger": "dev-master",
        "intervention/imagecache": "^2.3.4",
        "artesaos/seotools": "^0.10.0",
        "laravel/socialite": "^3.0",
        "consoletvs/charts": "6.*",
        "bestmomo/laravel5-3-installer": "dev-master",
        "barryvdh/laravel-omnipay": "0.3.*@dev",
        "omnipay/paypal": "*",
        "omnipay/stripe": "*",
        "laravel-notification-channels/onesignal": "^1.0",
        "socialiteproviders/steam": "^1.0",
        "socialiteproviders/twitch": "^3.0",
        "socialiteproviders/battlenet": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "fideloper/proxy": "^4.0",
        "laravelcollective/html": "^5.6",
        "spatie/laravel-cookie-consent": "^2.4",
        "cviebrock/eloquent-sluggable": "^4.5",
        "wiledia/backport": "dev-master",
        "wiledia/themes": "dev-master",
        "prologue/alerts": "^0.4.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laracasts/generators": "dev-master",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "bestmomo/laravel5-3-installer",
                "wiledia/backport"
            ]
        }
    }
}
